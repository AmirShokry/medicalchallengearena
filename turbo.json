{
	"$schema": "https://turborepo.com/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": [".nuxt/**", ".output/**", "dist/**"]
		},
		"lint": {},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"db:push": {
			"dependsOn": ["^db:push"],
			"cache": false
		}
	}
}
